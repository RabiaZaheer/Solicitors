@model Solicitor.Models.DefautViewModel

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutGFS.cshtml";
}

<div>   
    @Html.Partial("_TopServices", Model.TopServices)
</div>
<div>
    @Html.Partial("_TeamMembers", Model.TeamMembers)    
</div>

<div class="news-services row-fluid">
    <div class="left">@Html.Partial("_NewsSideBar",Model.News)</div>
    <div class="right">@Html.Partial("_AllServicesSideBar", Model.Services)</div>
</div>

<div>
    @Html.Partial("_Clients", Model.Clients)    
</div>
<div class="location-claim row-fluid">
    <div class="left">
        <h2>OUR LOCATION</h2>
        <div class="row-fluid">
            @Html.Partial("_Location")
        </div>
    </div>
    <div class="right">
        @Html.Partial("_Claim", Model.Claim)
    </div>
</div>
